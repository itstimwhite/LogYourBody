{
	"appId": "com.logyourbody.app",
	"appName": "LogYourBody",
	"webDir": "dist",
	"server": {
		"cleartext": true
	},
	"ios": {
		"scheme": "App",
		"contentInset": "automatic",
		"scrollEnabled": true,
		"backgroundColor": "#000000",
		"orientation": "portrait"
	},
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 0,
			"launchAutoHide": false,
			"backgroundColor": "#000000",
			"showSpinner": false,
			"androidSpinnerStyle": "large",
			"iosSpinnerStyle": "small",
			"spinnerColor": "#ffffff",
			"splashFullScreen": true,
			"splashImmersive": true,
			"fadeInDuration": 0,
			"fadeOutDuration": 200
		},
		"StatusBar": {
			"style": "light",
			"backgroundColor": "#000000"
		}
	},
	"packageClassList": [
		"SignInWithApple",
		"HapticsPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"CapacitorHealthkitPlugin"
	]
}
