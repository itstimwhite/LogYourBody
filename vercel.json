{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- .",
  "functions": {
    "**/*": {
      "excludeFiles": "legacy-react/**"
    }
  },
  "git": {
    "deploymentEnabled": {
      "dev": false,
      "main": false,
      "preview": false
    }
  }
}