# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Apple Developer Account
APPLE_ID_EMAIL=your-apple-id@example.com
APPLE_TEAM_ID=YOUR_TEAM_ID
APP_STORE_APP_ID=YOUR_APP_STORE_APP_ID

# App Configuration
APP_IDENTIFIER=com.logyourbody.app

# Code Signing
MATCH_PASSWORD=your-match-password
MATCH_GIT_URL=https://github.com/your-org/certificates.git
IOS_PROVISIONING_PROFILE_NAME=YOUR_PROFILE_NAME

# App Store Connect
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=your-app-specific-password

# CI/CD
FASTLANE_USER=$APPLE_ID_EMAIL
FASTLANE_PASSWORD=$FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD

# Optional: Skip 2FA on CI
FASTLANE_SKIP_TWO_FACTOR_AUTH=true
FASTLANE_SESSION=your-session-token